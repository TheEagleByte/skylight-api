{
  "name": "skylight-api",
  "version": "1.0.0",
  "description": "Convert HAR files to OpenAPI specification for Skylight API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "skylight-har2openapi": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "convert": "tsx src/index.ts convert",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist docs/openapi"
  },
  "keywords": [
    "skylight",
    "har",
    "openapi",
    "swagger",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "deepmerge": "^4.3.1",
    "glob": "^10.3.10",
    "har-to-openapi": "^2.2.1",
    "openapi-types": "^12.1.3",
    "yaml": "^2.3.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^20.12.2",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
